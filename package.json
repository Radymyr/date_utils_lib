{
  "devDependencies": {
    "typescript": "^5.6.2",
    "vitest": "^2.1.1"
  },
  "name": "date_utils_lib",
  "version": "1.0.1",
  "main": "src/index.js",
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir build/cjs",
    "watch": "npm run watch:esm && npm run watch:cjs",
    "watch:esm": "tsc --watch",
    "watch:cjs": "tsc --module commonjs --outDir build/cjs --watch",
    "dev": "tsc src/index.ts --watch",
    "test": "vitest run"
  },
